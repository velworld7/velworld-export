{
    "routes": [
        {
            "src": "/sitemap.xml",
            "dest": "/sitemap.xml"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/sitemap.xml",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/xml"
                }
            ]
        }
    ]
}